{
    "React boilerplate": {
        "prefix": "rc",
        "body": [
            "import React, {",
            "\tComponent,",
            "\tPropTypes,",
            "} from 'react';",
            "\n",
            "export default class $1 extends Component {",
            "\trender() {",
            "\t\treturn <div>Hello</div>;",
            "\t}",
            "}"
        ],
        "description": "React class boilerplate"
    },
    "Import": {
        "prefix": "imp",
        "body": [
            "import ${1:name} from '../${1:name}/';\n"
        ],
        "description": "Imports a module"
    }
}
